<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>


<VBox stylesheets="@NewFile.css" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="quizzbus.view.compte.ViewCompteList">
   <children>
      <HBox prefHeight="100.0" prefWidth="200.0" stylesheets="@NewFile.css">
         <children>
            <Label alignment="CENTER" styleClass="titre-vue" text="Comptes" />
         </children>
      </HBox>
      <HBox alignment="CENTER">
         <children>
            <ListView fx:id="lsvComptes" layoutX="88.0" layoutY="75.0" onMouseClicked="#gererClicSurListe" stylesheets="@NewFile.css">
               <opaqueInsets>
                  <Insets />
               </opaqueInsets>
            </ListView>
         </children>
      </HBox>
      <HBox alignment="CENTER" prefHeight="60.0" spacing="10.0">
         <children>
            <Button mnemonicParsing="false" onAction="#doAjouter" text="Ajouter" />
            <Button fx:id="btnModifier" mnemonicParsing="false" onAction="#doModifier" text="Modifier" />
            <Button fx:id="btnSupprimer" mnemonicParsing="false" onAction="#doSupprimer" text="Supprimer" />
         </children>
      </HBox>
   </children>
</VBox>
